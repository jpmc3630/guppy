{
  "name": "quasar",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Guppy",
  "author": "James McGlone <jamespmcglone@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "prod": "cd quasar; quasar build; cp -v ./dist/manifest.json ../public/; [ -d './dist/assets' ] && cp -va ./dist/assets ../public/; [ -d './dist/icons' ] &&  cp -va ./dist/icons ../public/; cd ..",
    "dev": "npm run prod",
    "watch": "cd quasar; quasar dev"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@quasar/quasar-ui-qcalendar": "^4.0.0-beta.15",
    "axios": "^0.21.1",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0-alpha.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
